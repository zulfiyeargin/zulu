{
    "manifest_version": 2,
    "name": "zulu",
    "description": "Don't forget what you searched",
    "version": "1.0",

    "browser_action" : {
        "default_icon" : "icons/icon.png"
    },

    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ]
}
